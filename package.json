{
  "name": "bd.tracker",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "wss:start": "node socket/server.ts",
    "prod": "next build && next start",
    "build": "next build",
    "build:with-ws": "NODE_ENV=production ts-node server.ts",
    "start": "next start",
    "lint": "next lint",
    "types": "tsc",
    "format": "prettier \"**/*.{js,cjs,mjs,jsx,ts,tsx,json}\" -w --config ./config/prettier/.prettierrc --ignore-path ./config/prettier/.prettierignore",
    "prepare": "husky",
    "test:unit": "jest --config ./config/jest/jest.config.ts --testMatch '**/?(*.)+(test).[jt]s?(x)'",
    "test:unit:watch": "jest --config ./config/jest/jest.config.ts --watch --testMatch '**/?(*.)+(test).[jt]s?(x)'",
    "test:int": "jest --config ./config/jest/jest.config.ts --testMatch '**/?(*.)+(itest).[jt]s?(x)'",
    "test:int:watch": "jest --config ./config/jest/jest.config.ts --watch --testMatch '**/?(*.)+(itest).[jt]s?(x)'",
    "test:e2e": "cypress open -C ./config/cypress/cypress.config.ts",
    "ui:add": "bun x shadcn@latest add",
    "storybook:dev": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "db:pg:format": "prisma format --schema ./config/prisma/schema.prisma",
    "db:pg:validate": "prisma validate --schema ./config/prisma/schema.prisma",
    "db:pg:generate": "prisma generate --schema ./config/prisma/schema.prisma",
    "db:pg:seed": "prisma db seed",
    "db:pg:migrate:dev": "prisma migrate dev --schema ./config/prisma/schema.prisma --name",
    "db:pg:migrate:prod": "npx prisma migrate deploy --schema ./config/prisma/schema.prisma",
    "db:pg:reset": "prisma migrate reset --schema ./config/prisma/schema.prisma",
    "docker:build": "docker-compose -f ./config/docker/docker-compose.yml build",
    "docker:build:override": "docker-compose -f ./config/docker/docker-compose.yml -f ./config/docker/docker-compose.override.yml build",
    "docker:build:local": "docker-compose -f ./config/docker/docker-compose-local.yml build",
    "docker:up": "docker-compose -f ./config/docker/docker-compose.yml up -d",
    "docker:up:override": "docker-compose -f ./config/docker/docker-compose.yml -f ./config/docker/docker-compose.override.yml up -d",
    "docker:up:local": "docker-compose -f ./config/docker/docker-compose-local.yml up -d",
    "docker:down": "docker-compose -f ./config/docker/docker-compose.yml down",
    "docker:down:override": "docker-compose -f ./config/docker/docker-compose.yml -f ./config/docker/docker-compose.override.yml down",
    "docker:down:local": "docker-compose -f ./config/docker/docker-compose-local.yml down"
  },
  "prisma": {
    "seed": "tsx ./config/prisma/seeds/seed.ts"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,json,css,scss,md}": [
      "prettier --write --config ./config/prettier/.prettierrc --ignore-path ./config/prettier/.prettierignore"
    ]
  },
