{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "sourceMap": true,
    "types": ["cypress", "node"],
    "esModuleInterop": true,

    "paths": {
      "@/*": ["../../src/*"],
      "$/*": ["../../api/*"],
      "#/*": ["../../app/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
